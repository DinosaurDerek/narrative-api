generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Brief {
  id        String   @id @default(uuid())
  date      DateTime
  topic     String
  content   Json
  createdAt DateTime @default(now())

  @@unique([date, topic], name: "date_topic")
}
